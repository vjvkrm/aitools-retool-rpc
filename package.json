  "description": "A library to use RPC  Retool for  OpenAI and other llm function calling",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "lint": "eslint ."
  },
  "keywords": [
    "openai",
    "retool",
    "rpc",
    "api"
  ],
  "author": "Vijay Vikram Singh",
  "license": "MIT",
  "dependencies": {
    "dotenv": "^16.4.5",
    "openai": "^4.63.0",
    "retoolrpc": "^0.1.6"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.4",
    "@babel/preset-typescript": "^7.24.7",
    "babel-jest": "^29.7.0",
    "eslint": "^8.40.0",
    "jest": "^29.7.0"
  },
  "jest": {
    "transform": {},
    "testEnvironment": "node",
    "extensionsToTreatAsEsm": [],
    "moduleNameMapper": {
